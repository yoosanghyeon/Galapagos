<?xml version="1.0" encoding="utf-8"?><!--네비게이션 메뉴-->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/navi_user_pic"
            android:layout_width="66dp"
            android:layout_height="66dp"
            android:layout_marginBottom="24dp"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="30dp"
            android:src="@drawable/user_img_66dp" />

        <TextView
            android:id="@+id/navi_user_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="100dp"
            android:paddingTop="32dp"
            android:text="Name"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/navi_user_location"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/navi_user_name"
            android:paddingLeft="100dp"
            android:text="설정한 지역: 서울대학교"
            android:textSize="12sp" />
    </RelativeLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="8dp"
        android:background="#bbbcbd"></LinearLayout>

    <LinearLayout
        android:id="@+id/notifi_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/text_notifi"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/navi_prifile"
            android:background="@drawable/textlinebold"
            android:padding="10dp"
            android:text="공지사항"
            android:textSize="15sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/profileInfo_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/text_profileInfo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/navi_prifile"
            android:background="@drawable/textlinebold"
            android:padding="10dp"
            android:text="프로필 정보"
            android:textSize="15sp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/textlinebold"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/navi_prifile"
            android:padding="10dp"
            android:text="지역 소모임보기"
            android:textColor="#757575"
            android:textSize="15sp" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/navi_prifile"
            android:layout_gravity="right"
            android:gravity="right"
            android:padding="10dp"
            android:paddingRight="20dp"
            android:text="ComingSoon!"
            android:textColor="#757575"
            android:textSize="15sp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/textlinebold"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/navi_prifile"
            android:padding="10dp"
            android:text="내 친구 찾기"
            android:textColor="#757575"
            android:textSize="15sp" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/navi_prifile"
            android:layout_gravity="right"
            android:gravity="right"
            android:padding="10dp"
            android:paddingRight="20dp"
            android:text="ComingSoon!"
            android:textColor="#757575"
            android:textSize="15sp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="8dp"
        android:background="#bbbcbd"></LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/textlinebold"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/navi_prifile"
            android:padding="10dp"
            android:text="버전정보"
            android:textSize="15sp" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/navi_prifile"
            android:layout_gravity="right"
            android:gravity="right"
            android:padding="10dp"
            android:paddingRight="20dp"
            android:text="V 0.0.1"
            android:textSize="15sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/licenseInfo_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/text_licenseInfo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/navi_prifile"
            android:background="@drawable/textlinebold"
            android:padding="10dp"
            android:text="라이센스 정보"
            android:textSize="15sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/question_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/text_question"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/navi_prifile"
            android:background="@drawable/textlinebold"
            android:padding="10dp"
            android:text="문의 하기"
            android:textSize="15sp" />
    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/textlinebold"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/navi_prifile"
            android:padding="10dp"
            android:text="푸쉬알림"
            android:textSize="15sp" />

        <android.support.v7.widget.SwitchCompat
            android:id="@+id/btn_navi_push"
            android:checked="true"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="right"
            android:padding="5dp"
            android:paddingRight="20dp" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="8dp"
        android:background="#bbbcbd"></LinearLayout>


</LinearLayout>
